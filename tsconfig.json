{
  "compileOnSave": false,
  "compilerOptions": {
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "ES2022",
      "dom"
    ],
    "module": "ES2022",
    "baseUrl": "./",
    "paths": {
      "lacuna-image-viewer": [
        "./dist/lacuna-image-viewer"
      ],
      "lacuna-image-viewer/*": [
        "./dist/lacuna-image-viewer/*"
      ]
    },
    "useDefineForClassFields": false
  }
}
